{
  "manifest_version": 2,
  "name": "Code Formatter",
  "version": "1.0",
  "description": "Format code blocks in Canvas for better readability.",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.instructure.com/courses/*/pages/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
